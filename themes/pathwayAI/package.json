{
  "name": "gulp",
  "version": "1.0.0",
  "description": "Gulp with Tailwind",
  "main": "gulpfile.js",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "autoprefixer": "^10.4.8",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^4.1.3",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "log-symbols": "^4.0.0",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "sass": "^1.54.4",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "tailwindcss": "^3.1.8",
    "unfetch": "^4.2.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "scripts": {
    "accessibility": "pa11y-ci --reporter=pa11y-ci-reporter-html --sitemap $npm_config_url/sitemap.xml",
    "build": "cross-env NODE_ENV=production gulp prod",
    "dev": "cross-env NODE_ENV=development gulp",
    "format": "run-p format:*",
    "format:css": "npx prettier '**/*.scss' --write && npm run lint:css -- --fix",
    "format:js": "npm run lint:js -- --fix",
    "lint": "run-p lint:*",
    "lint:css": "stylelint '**/*.scss'",
    "lint:js": "eslint **/*.js"
  },
  "dependencies": {
    "countup.js": "^2.3.2",
    "flickity": "^3.0.0",
    "gsap": "^3.11.2",
    "isotope-layout": "^3.0.6",
    "lazysizes": "^5.3.2",
    "micromodal": "^0.4.10",
    "photoswipe": "^4.1.3"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ]
}
